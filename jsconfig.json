{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@view/*": ["./src/view/*"],
      "@services/*": ["./src/services/*"],
      "@utils/*": ["./src/utils/*"],
      "@js/*": ["./src/assets/js/*"],
      "@css/*": ["./src/assets/css/*"],
      "@img/*": ["./src/assets/img/*"],
      "@config/*": ["./src/config/*"],
      "@router/*": ["./src/router/*"],
      "@store/*": ["./src/store/*"],
      "@mixin/*": ["./src/mixins/*"]
    }
  }
}