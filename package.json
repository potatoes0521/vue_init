{
  "name": "vue_cli_init",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "log": "conventional-changelog --config ./node_modules/ly-tool-git-commitlint/lib/log -i CHANGELOG.md -s -r 0",
    "cz": "npm run log && git add . && git cz",
    "test": "vue-cli-service build --mode test",
    "pro": "vue-cli-service build --mode pro"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/ly-tool-git-commitlint/lib/cz"
    }
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.0.6",
    "axios": "^0.21.1",
    "compression-webpack-plugin": "5.0.1",
    "core-js": "^3.6.5",
    "crypto-js": "^4.0.0",
    "element-ui": "^2.15.1",
    "image-webpack-loader": "^7.0.1",
    "js-cookie": "^2.2.1",
    "loadsh": "^0.0.4",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.0",
    "pug": "^3.0.2",
    "pug-html-loader": "^1.1.5",
    "pug-plain-loader": "^1.1.0",
    "right-pad": "^1.0.1",
    "screenfull": "^5.1.0",
    "vue": "^2.6.11",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitizen": "4.2.3",
    "commitlint": "12.0.1",
    "conventional-changelog-cli": "2.1.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "@3.0.9",
    "ly-tool-git-commitlint": "1.1.15",
    "prettier": "^1.19.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  }
}